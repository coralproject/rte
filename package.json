{
  "name": "@coralproject/rte",
  "version": "2.2.2",
  "description": "Coral Rich Text Editor",
  "author": "The Coral Project Team @coralproject",
  "license": "Apache-2.0",
  "homepage": "https://github.com/coralproject/rte",
  "files": [
    "lib",
    "cjs"
  ],
  "module": "./lib/index.js",
  "main": "./cjs/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:lib": "rm -rf ./lib && NODE_ENV=production tsc --declaration --sourceMap --noEmit false --outDir lib --module es6 && copyfiles -u 1 './src/**/*.css' lib",
    "build:cjs": "rm -rf ./cjs && NODE_ENV=production tsc --declaration --sourceMap --noEmit false --outDir cjs --module commonjs && copyfiles -u 1 './src/**/*.css' cjs",
    "build:docs": "rm -rf ./docs/dist && node docs/scripts/copy.js && webpack --config docs/webpack.config.js --context docs/",
    "dev": "npm run build:docs && webpack serve --config docs/webpack.config.js --context docs/",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}' './docs/**/*.tsx'",
    "lint:tsc": "tsc --noEmit",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^16.2.5",
    "@types/lodash": "^4.14.166",
    "@types/node": "^18.16.0",
    "@types/nunjucks": "^3.2.2",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copyfiles": "^2.4.1",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.18.2",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "prettier": "^2.2.1",
    "prism-react-renderer": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.13.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "squire-rte": "1.11.0"
  },
  "dependencies-pins-documentation": {
    "squire-rte@1.1.0": [
      "Newer versions have problems with line breaks in iOS.",
      "Update when the following issue is resolved:",
      "https://github.com/neilj/Squire/issues/413"
    ]
  },
  "browsers": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "IE 11",
    "not dead"
  ]
}
