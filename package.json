{
  "name": "@coralproject/rte",
  "version": "1.2.0",
  "description": "Coral Rich Text Editor",
  "author": "The Coral Project Team <coral@mozillafoundation.org>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/coralproject/rte",
  "files": [
    "lib",
    "cjs"
  ],
  "module": "./lib/index.js",
  "main": "./cjs/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:lib": "rm -rf ./lib && NODE_ENV=production tsc --declaration --sourceMap --noEmit false --outDir lib --module es6",
    "build:cjs": "rm -rf ./cjs && NODE_ENV=production tsc --declaration --sourceMap --noEmit false --outDir cjs --module commonjs",
    "build:docs": "rm -rf ./docs/dist && webpack --config docs/webpack.config.js --context docs/",
    "dev": "webpack-dev-server --config docs/webpack.config.js --context docs/",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint ./src/*.{ts,tsx} ./src/**/*.{ts,tsx} ./docs/**/*.tsx ./docs/*.tsx",
    "lint:tsc": "tsc --noEmit",
    "release": "npm version -m '@coralproject/rte release %s'"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@coralproject/mdx-book": "^0.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jsdom": "^16.2.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "squire-rte": "^1.9.0"
  },
  "browsers": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "IE 11",
    "not dead"
  ]
}
